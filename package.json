{
  "name": "sveltestrap",
  "version": "0.8.0",
  "main": "dist/sveltestrap.js",
  "keywords": [
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:bestguy/sveltestrap.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dist": "rollup -c",
    "docs": "rollup -c -i src/Docs.html -o docs/index.js -f iife -n Docs",
    "prepublish": "npm run docs; npm run dist",
    "postpublish": "git push && git push --tags",
    "watch": "rollup -c -w -i src/Docs.html -o docs/index.js -f iife -n Docs"
  },
  "dependencies": {
    "svelte": "^1.12.1"
  },
  "devDependencies": {
    "lodash.tonumber": "^4.0.3",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-svelte": "^1.6.1",
    "rollup-watch": "^3.2.2"
  }
}
