{
	"name": "sveltestrap",
	"version": "1.0.0",
	"main": "dist/sveltestrap.js",
	"module": "dist/sveltestrap.es.js",
	"jsnext:main": "dist/sveltestrap.es.js",
	"svelte": "src/index.js",
	"keywords": [
		"svelte"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:bestguy/sveltestrap.git"
	},
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"start": "webpack-dev-server --config ./webpack.docs.config.js --watch --hot",
		"build": "npm run dist",
		"build:docs": "cross-env WEBPACK_BUILD=production webpack --config ./webpack.docs.config.js --progress --colors",
		"dist": "rollup -c",
		"prepublish": "npm run dist",
		"postpublish": "git push && git push --tags",
		"version": "npm run docs && git add -A docs",
		"watch": "rollup -c -w",
		"postinstall": "npx patch-package"
	},
	"peerDependencies": {
		"svelte": "^3.4.4"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"babel-loader": "^8.0.6",
		"bootstrap": "^4.3.1",
		"clean-webpack-plugin": "^3.0.0",
		"clsx": "^1.0.4",
		"copy-webpack-plugin": "^5.0.3",
		"cross-env": "^5.2.0",
		"json-loader": "^0.5.7",
		"lodash.tonumber": "^4.0.3",
		"mini-css-extract-plugin": "^0.7.0",
		"raw-loader": "^2.0.0",
		"rollup": "^1.12.4",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-commonjs": "^10.0.0",
		"rollup-plugin-node-resolve": "^5.0.0",
		"rollup-plugin-peer-deps-external": "^2.2.0",
		"rollup-plugin-svelte": "^5.0.3",
		"static-site-generator-webpack-plugin": "^3.4.2",
		"svelte": "^3.4.4",
		"svelte-loader": "^2.13.4",
		"webpack": "^4.32.2",
		"webpack-cli": "^3.3.2",
		"webpack-dev-server": "^3.4.1"
	},
	"dependencies": {
		"lodash.isobject": "^3.0.2"
	}
}
